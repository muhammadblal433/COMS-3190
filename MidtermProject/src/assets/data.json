        {
            "recipeId": "R009",
            "title": "Crepes",
            "year": 2023,
            "img": "../assets/images/recipes/crepe.png",
            "price": 4.00,
            "ingredients": [
                "1 cup all-purpose flour",
                "2 eggs",
                "1 cup milk",
                "1/4 cup water",
                "2 tbsp butter (melted)",
                "1 tbsp sugar",
                "1/4 tsp salt"
            ],
            "instructions": "Whisk all ingredients together until smooth. Heat a lightly oiled pan over medium heat. Pour in batter and swirl to coat the pan. Cook for 1-2 minutes per side until golden. Fill with desired toppings and serve."
